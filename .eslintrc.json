{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["airbnb", "plugin:jsx-a11y/recommended", "prettier"],
  "parserOptions": {
    "sourceType": "module"
  },
  "plugins": ["jsx-a11y"],
  "rules": {
    "semi": "error"
  },
  "ignorePatterns": [
    "node_modules",
    "build",
    "package.json",
    "package-lock.json"
  ]
}
